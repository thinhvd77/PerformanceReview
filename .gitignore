# Root .gitignore for PerformanceReviewFormManagement
# Node.js
node_modules/
**/node_modules/

# Logs
logs/
*.log
*.log.*
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*

# Runtime data
pids/
*.pid
*.seed
*.pid.lock

tmp/
temp/
**/tmp/
**/temp/
.cache/
.eslintcache

# Build outputs
build/
dist/
**/build/
**/dist/

# Coverage
coverage/
.nyc_output/

# Environment files
.env
.env.*
!.env.example
**/.env
**/.env.*
!**/.env.example

# Uploads and generated files
uploads/
**/uploads/

# OS specific
.DS_Store
Thumbs.db

# IDE/editor
.idea/
.vscode/
*.iml

# Databases (if any local sqlite used)
*.sqlite
*.sqlite3
*.db

# Excel temporary lock files
~$*.xls
~$*.xlsx

# Misc
*.swp
*.swo
