{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend API with Express and TypeORM",
  "main": "src/app.js",
    "type": "module",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "typeorm": "node ./node_modules/typeorm/cli.js -d ./src/config/database.js",
    "migration:generate": "npm run typeorm -- migration:generate --outputJs",
    "migration:run": "npm run typeorm -- migration:run"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "pg": "^8.11.3",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
